{
    "installBehavior": {
        "disableInstallNotes": true,
        "portablePackageUserRoot": "C:/Users/decipher/Apps",
        "preferences": {
            "locale": [
                "en-US"
            ]
        }
    },
    "uninstallBehavior": {
        "purgePortablePackage": true
    },
    "network": {
        "downloader": "wininet"
    },
    "telemetry": {
        "disable": true
    },
    "visual": {
        "anonymizeDisplayedPaths": true
    },
    "experimentalFeatures": {
        "configureSelfElevate": true,
        "directMSI": true,
        "fonts": true
    }
}